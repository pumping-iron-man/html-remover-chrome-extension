{
    "manifest_version": 2,
    "name": "Page-Elements-Remover",
    "version": "0.1",
    "description": "Remove any annoying html element/section on your current webpage",
    "content_scripts": [
        {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
        }
    ],
    "background": {
        "scripts" : ["background.js"],
        "persistent" : false
    },
    "browser_action": {},
    "icons" : {
        "16" : "images/removeIconOFF_16.png",
        "32" : "images/removeIconOFF_32.png",
        "48" : "images/removeIconOFF_48.png",
        "128" : "images/removeIconOFF_128.png"
    }
}